{
  "schemaVersion": 1,
  "id": "emixx",
  "version": "${version}",
  "name": "EMI++",
  "description": "Adds many useful features to EMI!",
  "authors": [
    "ConcerroX",
    "evanbones"
  ],
  "contact": {
    "homepage": "https://github.com/ConcerroX/emi-plus-plus/",
    "sources": "https://github.com/ConcerroX/emi-plus-plus/"
  },
  "license": "MIT",
  "icon": "icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "concerrox.emixx.fabric.EmiPlusPlusFabric"
      }
    ],
    "client": [
      {
        "adapter": "kotlin",
        "value": "concerrox.emixx.fabric.EmiPlusPlusClientFabric"
      }
    ],
    "kubejs": [
      "concerrox.emixx.integration.kubejs.EmiPlusPlusKubeJSPlugin"
    ]
  },
  "mixins": [
    "emixx-common.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.14.25",
    "minecraft": "~1.20.1",
    "java": ">=17",
    "fabric-language-kotlin": ">=1.13.3+kotlin.2.1.21",
    "forgeconfigapiport": ">=8.0.0"
  },
  "accessWidener": "emixx-common.accesswidener"
}
